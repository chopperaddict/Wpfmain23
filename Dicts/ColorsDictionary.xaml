			StartPoint="0.4 0" EndPoint="0.5 1">
        <GradientStop Offset="0" Color="#FF000000" />
        <GradientStop Offset="4" Color="#FF000000" />
        <GradientStop Offset="0.5" Color="{DynamicResource ClrCyan5}" />
